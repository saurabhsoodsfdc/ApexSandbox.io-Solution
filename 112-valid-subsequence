public Boolean isSubSequence(String s1, String s2){
   
   if(s2.length () < s1.length()) {
       return false;
   }

   Integer i = 0; 
   Integer j = 0;

   while(i < s1.length() && j < s2.length()) {

       if(s1.charAt(i) == s2.charAt(j)) {
           i++;
       }
       j++;
   }
   return i == s1.length();
}